<?xml version="1.0" encoding="UTF-8" ?>
<templates>
    <t t-name="owl.PdfTransfertListView.Buttons" t-inherit="web.ListView.Buttons" owl="1">
        <xpath expr="//button[hasclass('o_list_button_add')]" position="after">

            <button t-on-click="()=>this.printTransferts(this.model.root.selection)" t-if="nbSelected" class="ms-2 btn btn-outline-info" type="button">
                <i class="fa fa-print"></i> Imprimer
            </button>

        </xpath>
        
    </t>
</templates>